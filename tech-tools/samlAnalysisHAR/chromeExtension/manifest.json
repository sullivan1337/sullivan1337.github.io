{
  "manifest_version": 3,
  "name": "SSO Troubleshooter",
  "version": "1.0",
  "description": "An extension to decode and analyze SAML, HAR, and other SSO traces for easier troubleshooting.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}